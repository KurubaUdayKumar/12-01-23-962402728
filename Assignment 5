  student()
  {
 
   name="unknown";
      age=0;
    add="not available";  
  }
  
 setinfo(char nam[20], int ag)
  {
   name=nam;
      age=ag;
   cout<<"Details of student is"<<endl; 
   cout<<"Name : "<<name<<endl;
   cout<<"Age : "<<age<<endl;
  }
  
 setinfo(string nam, int ag, string addres)
  {
   name = nam;
      age= ag;
    add=addres; 
  }
 void display()
  { 
    cout<<"Name : "<<name<<endl;
    cout<<"Age : "<<age<<endl;
    cout<<"Address : "<<add<<endl;
  }
};
